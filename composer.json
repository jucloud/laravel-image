{
  "name": "sdclub/laravel-image",
  "description": "laravel-image 是一个辅助服务，处理上传的图像和存储图像没有重复.",
  "keywords": [
    "image",
    "service",
    "laravel-image",
    "starter-framework",
    "laravel-starter-framework",
    "laravel"
  ],
  "homepage": "https://github.com/sdclub/laravel-image",
  "license": "MIT",
  "authors": [
    {
      "name": "Jamie",
      "email": "jamie@sdclub.org",
      "homepage": "https://www.sdclub.org/"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "laravel/framework": "5.2.*|5.3.*|5.4.*",
    "intervention/image": "^2.3",
    "intervention/imagecache": "~2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "files": [
      "src/App/Image/Foundation/helpers.php"
    ],
    "psr-4": {
      "App\\Image\\": "src/App/Image"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Image\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
